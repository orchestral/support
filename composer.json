{
	"name": "orchestra/support",
	"description": "Orchestra Platform 2 Support Component",
	"homepage": "http://orchestraplatform.com/docs/latest/components/support/",
	"keywords": ["orchestra-platform", "orchestral", "laravel", "messages", "notification", "validator", "utilities"],
	"license": "MIT",
	"authors": [
		{
			"name": "Mior Muhammad Zaki",
			"email": "crynobone@gmail.com",
			"homepage": "https://github.com/crynobone"
		}
	],
	"autoload": {
		"psr-4": {
			"Orchestra\\Support\\" : "src/Support/"
		}
	},
	"require": {
		"php": ">=5.4.0"
	},
	"require-dev": {
		"mockery/mockery": "0.9.*",
		"patchwork/utf8": "1.1.*",
		"illuminate/database": "4.2.*",
		"illuminate/events": "4.2.*",
		"illuminate/session": "4.2.*",
		"illuminate/validation": "4.2.*",
		"illuminate/support": "4.2.*"
	},
	"suggest": {
		"laravel/framework": "Allow using orchestra/support component outside of Orchestra Platform 2",
		"orchestra/foundation": "Allow using orchestra/support with Orchestra Platform 2"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.3-dev"
		}
	},
	"minimum-stability": "dev"
}
